<header class="header">
  <div class="titles">
  <h1 class="titles__title"></h1>
  </div>
  
</header>
   <body>
      <%= render partial: "header" %>
      <div class="middle">
        <%= render partial: "selectleft" %>
        <ul class="pianocenter">
          <ul class="posts"></ul>
            <ul class="posts__title">Piano Post</ul>
              <div class="contents">              
                  <% @tweets.each do |tweet| %>
                <div class="contentss">
                  <span class="contents__name">
                  <%= tweet.name %>
                    <div class="post">
                      <div class="post__image">
                      <%= video_tag tweet.video_url.to_s,loop: true, controls: true, width: 550, id: "mv" %>
                      </div> 
                    </div>                                                     
                  </span>
                </div>
                  <% end %>
                 
              </div>    
            <%= paginate(@tweets) %> 
        </ul>
        <%= render partial: "selectright" %>
      </div>
      <%= render partial: "footer" %>
  </body>
</html>

 